{
  "name": "trogers1884/laravel-timer",
  "description": "A simple timer utility for Laravel applications",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Trogers1884\\LaravelTimer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trogers1884\\LaravelTimer\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trogers1884\\LaravelTimer\\TimerServiceProvider"
      ],
      "aliases": {
        "Timer": "Trogers1884\\LaravelTimer\\Facades\\Timer"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}